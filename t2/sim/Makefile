# ------------------------------------------------------------
# type "make" command in the Linux terminal create report.pdf
#
# type "make clean" to delete all generated files
# ------------------------------------------------------------

all:
	make -C 1
	make -C 2
	make -C 3
	make -C 4
	make -C 5

clean:
	make -C 1 clean
	make -C 2 clean
	make -C 3 clean
	make -C 4 clean
	make -C 5 clean

.PHONY: all clean


